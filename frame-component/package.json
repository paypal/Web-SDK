{
  "name": "frame-component",
  "version": "0.0.1",
  "description": "[WIP] One merchant integration point for all of PayPal's services",
  "main": "index.js",
  "private": true,
  "scripts": {
    "demo": "run-p demo:*",
    "demo:parent-page": "vite --config demo/parent/vite.config.ts serve demo/parent",
    "demo:child-page": "vite --config demo/child/vite.config.ts serve demo/child",
    "build": "tsc --declaration",
    "lint": "eslint .",
    "test": "jest",
    "test:e2e": "npm run demo & npm run wdio",
    "prewdio": "wait-on http://localhost:3000/index.html && wait-on http://localhost:3001/index.html",
    "wdio": "wdio run ./e2e-tests/wdio.conf.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/paypal/Web-SDK.git"
  },
  "keywords": [
    "paypal",
    "braintree",
    "payments"
  ],
  "author": "PayPal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paypal/Web-SDK/issues"
  },
  "homepage": "https://github.com/paypal/Web-SDK#readme",
  "devDependencies": {
    "@preact/preset-vite": "^2.1.7",
    "@wdio/cli": "^7.16.15",
    "@wdio/local-runner": "^7.16.15",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/spec-reporter": "^7.16.14",
    "chromedriver": "^98.0.0",
    "ts-node": "^10.5.0",
    "vite": "^2.7.13",
    "wait-on": "^6.0.1",
    "wdio-chromedriver-service": "^7.2.8",
    "wdio-wait-for": "^2.2.1"
  },
  "dependencies": {
    "@braintree/iframer": "^1.1.0",
    "@braintree/uuid": "^0.1.0",
    "framebus": "^6.0.0-rc.3",
    "preact": "^10.6.6"
  }
}
